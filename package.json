{
  "name": "my-community",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "19.0.0",
    "@ckeditor/ckeditor5-react": "2.1.0",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@tribeplatform/gql-client": "^1.0.6",
    "@tribeplatform/react-sdk": "^2.0.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "predeploy": "yarn build && yarn cp",
    "deploy": "gh-pages -d build",
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "set NODE_OPTIONS=--max_old_space_size=8192 && webpack --config webpack.prod.js",
    "build-linux": "export NODE_OPTIONS='--max-old-space-size=8192' && webpack --config webpack.prod.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cp": "cp ./build/index.html ./build/404.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15"
  }
}
